<Window x:Class="ClazyDesktop.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ClazyDesktop"
        mc:Ignorable="d"
        Title="Clazy" Height="670" Width="1034"
        xmlns:system="clr-namespace:System;assembly=mscorlib"
        Icon="pack://application:,,,/ClazyDesktop;component/Resources/ClazyCircle.ico"
        xmlns:viewModels="clr-namespace:ClazyDesktop.ViewModels"
        Style="{StaticResource CustomWindowStyle}"
        WindowStartupLocation="CenterScreen">
    <Window.DataContext>
        <viewModels:MainViewModel/>
    </Window.DataContext>
    
    <Window.Resources>
        <DropShadowBitmapEffect x:Key="NormalShadow" ShadowDepth="2" Softness="0.2" Opacity="0.64"/>
        <x:Array x:Key="temps" Type="{x:Type system:String}">
            <system:String>ello1</system:String>
            <system:String>ello2</system:String>
            <system:String>ello3</system:String>
            <system:String>ello4</system:String>
        </x:Array>
    </Window.Resources>
    
    <Grid>
        
    </Grid>
</Window>
